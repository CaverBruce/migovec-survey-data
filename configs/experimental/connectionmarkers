source 
	
	survey connection_colors

		centreline
		units length meters
    	units compass clino degrees
    	data normal from to length compass clino
    	flags duplicate
		1 2 100 0 0
		flags not duplicate
		endcentreline


		scrap connection_colors -scale [0 0 50 0 0 0 1 0 m] -projection plan

		point 0 0 u:connectionmarker_MY -attr code 0
		point 0 10 u:connectionmarker_MY -attr code 1
		point 0 20 u:connectionmarker_MY -attr code 2
		point 0 30 u:connectionmarker_MY -attr code 3
		point 0 40 u:connectionmarker_MY -attr code 4
		point 10 0 u:connectionmarker_MY -attr code 5
		point 10 10 u:connectionmarker_MY -attr code 6
		point 10 20 u:connectionmarker_MY -attr code 7
		point 10 30 u:connectionmarker_MY -attr code 8
		point 10 40 u:connectionmarker_MY -attr code 9
		point 20 0 u:connectionmarker_MY -attr code 10
		point 20 10 u:connectionmarker_MY -attr code 11
		point 20 20 u:connectionmarker_MY -attr code 12
		point 20 30 u:connectionmarker_MY -attr code 13
		point 20 40 u:connectionmarker_MY -attr code 14
		point 30 0 u:connectionmarker_MY -attr code 15
		point 30 10 u:connectionmarker_MY -attr code 16
		point 30 20 u:connectionmarker_MY -attr code 17
		point 30 30 u:connectionmarker_MY
		point 30 40 u:connectionmarker_MY -attr rbg (0,1,0)

		point 0 0 station 1 -visibility off
		point 0 100 station 2 -visibility off
		endscrap
	endsurvey
endsource