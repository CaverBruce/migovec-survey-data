survey brezno_pod_vrhom_planje -title "Brezno pod Vrhom Planje"
	
	input "./brezno_planja_ent/brezno_planja_ent.th"
	input "./okna/okna.th"

	equate 10@okna 35@brezno_planja_ent

	map m-all-p -projection plan
		m-all-p@brezno_planja_ent
	endmap

	map m-all-e -projection extended
		m-all-e@brezno_planja_ent
	endmap

	centreline
	cs epsg:3794
	fix 0@brezno_planja_ent 404177 125263 0 #1877
	endcentreline

endsurvey